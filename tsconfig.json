{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "dom"
    ],
    "allowJs": true,
    "outDir": "dist",
    "downlevelIteration": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@DataStructure/*": [
        "DataStructure/*"
      ],
      "@Algorithm/*": [
        "Algorithm/*"
      ],
      "@Entity/*": [
        "Entity/*"
      ],
      "@Interface/*": [
        "Interface/*"
      ],
      "@Utils/*": [
        "Utils/*"
      ]
    },
    "types": [
      "node",
      "jest"
    ],
    "esModuleInterop": true,
    "inlineSources": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}